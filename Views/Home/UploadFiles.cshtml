
@{
    ViewData["Title"] = "UploadFiles";
}

<h2>UploadFiles</h2>

<form asp-action="UploadFiles"
      asp-controller="Home"
      method="post"
      enctype="multipart/form-data">
    <input type="file" id="FileName" name="files" multiple />
    <input type="submit" value="Upload Selected Files" />
</form>

